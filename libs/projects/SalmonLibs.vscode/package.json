{
  "name": "salmonlibs.vscode",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "test": "jest --runInBand",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "eslint": "eslint ./test/**/*.js --ext .js",
    "docs": "jsdoc ./lib -r -d jsdoc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.21.0",
    "@eslint/compat": "^1.2.7",
    "eslint-plugin-import": "^2.31.0",
    "jsdoc": "^4.0.4",
    "npx": "^10.2.2",
	"tslint": "^6.1.3",
	"tslint-no-circular-imports": "^0.7.0"
  },
  "dependencies": {
    "jest": "^29.7.0",
    "jest-editor-support": "^31.1.2"
  },
  "jest": {
    "rootDir": "./test/",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "node",
    "testTimeout": 3000000,
    "testMatch": [
      "**/*.test.(js)"
    ],
    "maxWorkers": 1
  },
  "type": "module"
}
